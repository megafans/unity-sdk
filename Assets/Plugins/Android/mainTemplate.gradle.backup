}
([rootProject] + (rootProject.subprojects as List)).each { project ->
    project.repositories {
        def unityProjectPath = "file:///" + file(rootProject.projectDir.path + "/../../").absolutePath
        maven {
            url "https://maven.google.com"
        }
        maven {
            url "https://maven.google.com" // Assets/PlayServicesResolver/Editor/MFUnitySDKDependencies.xml:12, Assets/PlayServicesResolver/Editor/MFUnitySDKDependencies.xml:20
